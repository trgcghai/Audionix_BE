{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@decorators/*": ["src/common/decorators/*"],
      "@enums/*": ["src/common/enums/*"],
      "@filters/*": ["src/common/filters/*"],
      "@interceptors/*": ["src/common/interceptors/*"],
      "@guards/*": ["src/common/guards/*"],
      "@interfaces/*": ["src/common/interfaces/*"],
      "@templates/*": ["src/common/templates/*"],
      "@validators/*": ["src/common/validators/*"],
      "@common/*": ["src/common/*"],
      "@config/*": ["src/config/*"],
      "@albums/*": ["src/modules/albums/*"],
      "@artists/*": ["src/modules/artists/*"],
      "@auth/*": ["src/modules/auth/*"],
      "@playlists/*": ["src/modules/playlists/*"],
      "@redis/*": ["src/modules/redis/*"],
      "@tracks/*": ["src/modules/tracks/*"],
      "@upload/*": ["src/modules/upload/*"],
      "@users/*": ["src/modules/users/*"],
      "@modules/*": ["src/modules/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
